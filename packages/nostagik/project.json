{
  "name": "@nostagik/core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/nostagik/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/packages/core",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "dist/packages/core",
        "command": "npm publish"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ]
    }
  }
}
